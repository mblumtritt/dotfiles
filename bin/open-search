#! /bin/sh
USAGE="Usage: ${0##*/} ENGINE TERM

Open search ENGINE with TERM."

set -e
case "$1" in
('') echo "${0##*/}: argument missing" >&2 && exit 1 ;;
(-h|--help) echo "$USAGE" ;;
(*)
  [ $# -lt 2 ] && echo "${0##*/}: TERM missing" >&2 && exit 1
  url="$(print-search-url "$@")" || exit $?
  open "$url"
;;
esac
