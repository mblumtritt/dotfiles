#! /bin/sh
USAGE="Usage: ${0##*/}

Clean up Docker stuff."

set -e
case "$1" in
('')
  start-docker
  docker image prune -f
  docker container prune -f
  docker volume prune -f
  docker network prune -f
  docker system prune -f -a
;;
(-h|--help) echo "$USAGE" ;;
(*) echo "${0##*/}: invalid argument - $1" >&2 ; exit 1 ;;
esac
